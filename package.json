{
  "name": "@itrulia/nova",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Angular Component Library for Statistics",
  "homepage": "https://github.com/itrulia/nova",
  "bugs": "https://github.com/itrulia/nova/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/itrulia/nova.git"
  },
  "scripts": {
    "build": "ng-packagr -p ng-package.json && npm run typedoc && npm run compodoc",
    "test": "ng test --watch=false --code-coverage",
    "tdd": "ng test --watch=true --code-coverage",
    "compodoc": "compodoc -p public/tsconfig.app.json --output dist/angular-docs",
    "typedoc": "typedoc --mode modules --exclude \"**/+(node_modules|e2e|public|tests)/**\" --externalPattern \"**/+(node_modules|e2e|public|tests)/**/*.ts\" --module commonjs --target ES5 --name @itrulia/nova --mode modules --out dist/api-docs .",
    "lint": "tslint --project .",
    "e2e": "ng e2e",
    "prepublishOnly": "npm run build"
  },
  "private": true,
  "devDependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cli": "1.6.3",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.2.1",
    "@compodoc/compodoc": "^1.0.4",
    "@frctl/fractal": "^1.1.7",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "core-js": "^2.4.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "ng-packagr": "^1.6.0",
    "protractor": "~5.1.2",
    "puppeteer": "^0.13.0",
    "ramda": "^0.25.0",
    "rxjs": "^5.4.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.5.3",
    "zone.js": "^0.8.14"
  }
}
